#include <iostream>
#include <typeinfo>
using namespace std;

int main() {

	string value;
	cout<<typeid(value).name()<<endl;
	decltype(value) name = "Deepak";
	cout << name << endl;
// Use of auto
	auto intValue = 10;
	cout << intValue << endl;
	return 0;
}
